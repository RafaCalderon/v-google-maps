{
  "name": "v-google-maps",
  "version": "1.2.0",
  "private": true,
  "main": "dist/v-google-maps.umd.cjs",
  "module": "dist/v-google-maps.js",
  "browser": "dist/v-google-maps.js",
  "type": "module",
  "types": "src/typings/v-google-maps.d.ts",
  "files": [
    "dist/*.{js,cjs,mjs}",
    "src/typings/**/*.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./src/typings/v-google-maps.d.ts",
      "import": "./dist/v-google-maps.js",
      "require": "./dist/v-google-maps.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --declaration --project tsconfig.build.json && run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "format": "prettier --write src",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint \"src/**/*.{js,ts,vue}\""
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@googlemaps/markerclusterer": "^2.5.3",
    "fast-deep-equal": "^3.1.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vue-tsc": "^2.2.0"
  },
  "peerDependencies": {
    "vue": "^3"
  }
}
