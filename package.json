{
  "name": "v-google-maps",
  "version": "1.0.0",
  "private": true,
  "main": "dist/v-google-maps.umd.cjs",
  "module": "dist/v-google-maps.js",
  "browser": "dist/v-google-maps.js",
  "type": "module",
  "types": "src/typings/index.d.ts",
  "files": [
    "dist/*.{js,cjs,mjs}",
    "src/typings/*.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./src/typings/index.d.ts",
      "import": "./dist/v-google-maps.js",
      "require": "./dist/v-google-maps.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --declaration --project tsconfig.build.json && run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "format": "prettier --write src",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint \"src/**/*.{js,vue}\""
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@googlemaps/markerclusterer": "^2.5.3",
    "fast-deep-equal": "^3.1.3",
    "vue": "^3.4.34"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/google.maps": "^3.55.12",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.1.1",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0",
    "vite": "^5.3.5",
    "vue-tsc": "^2.0.29"
  },
  "peerDependencies": {
    "vue": "^3"
  }
}
